{
  "name": "lomap",
  "version": "1.0.0",
  "description": "api.blackoutgg.com",
  "main": "src/server.js",
  "scripts": {
    "seed": "node run seed:forms",
    "seed:forms": "node ./seeds/seedForms",
    "seed:users": "node ./seeds/seedUsersAndRoles",
    "test": "NODE_ENV=test knex seed:run && npm run jest",
    "jest": "jest --forceExit --detectOpenHandles --runInBand",
    "dev": "nodemon -r dotenv/config src/server.js",
    "start": "nodemon server",
    "rollback": "knex migrate:rollback",
    "populate": "knex seed:run --specific=categories.js && knex seed:run --specific=roles.js && knex seed:run --specific=settings.js && knex seed:run --specific=users.js && knex seed:run --specific=testimonies.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@godaddy/terminus": "^4.9.0",
    "@socket.io/redis-adapter": "^7.0.0",
    "@socket.io/redis-emitter": "^4.1.0",
    "aws-sdk": "^2.931.0",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "date-fns": "^2.22.1",
    "discord.js": "^14.0.3",
    "dotenv": "^8.6.0",
    "dynamodb-toolbox": "^0.3.5",
    "express": "^4.17.1",
    "express-jwt": "^6.1.0",
    "express-jwt-permissions": "^1.3.5",
    "express-pino-logger": "^4.0.0",
    "express-validator": "^6.11.0",
    "fdir": "^4.1.0",
    "helmet": "^4.6.0",
    "ioredis": "^4.27.6",
    "joi": "^17.6.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.2.0",
    "lodash.pick": "^4.4.0",
    "lodash.snakecase": "^4.1.1",
    "lodash.uniq": "^4.5.0",
    "lodash.uniqby": "^4.7.0",
    "module-alias": "^2.2.2",
    "multer": "^1.4.2",
    "multer-s3": "^2.10.0",
    "nanoid": "^3.1.23",
    "objection": "^3.0.1",
    "objection-cursor": "^1.2.6",
    "pg": "^8.7.3",
    "pg-range": "^1.1.0",
    "phin": "^3.6.0",
    "pluralize": "^8.0.0",
    "sanitize-html": "^2.4.0",
    "slugify": "^1.6.5",
    "socket.io": "^4.1.3",
    "uuid4": "^2.0.2"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFiles": [
      "dotenv/config"
    ],
    "moduleNameMapper": {
      "^@root(.*)$": "<rootDir>$1",
      "^@util(.*)$": "<rootDir>/src/util$1",
      "^@services(.*)$": "<rootDir>/src/services$1"
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "_moduleAliases": {
    "$root": ".",
    "$util": "./src/util",
    "$services": "./src/services",
    "$models": "./src/models",
    "$bot": "./src/bot"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.3.0",
    "blaver": "^6.6.7",
    "html-to-text": "^8.1.0",
    "jest": "^27.4.5",
    "minimist-lite": "^2.2.1",
    "mjml": "^4.11.0",
    "playwright": "^1.24.1",
    "supertest": "^4.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BlackoutGG/api.blackoutgg.com.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/BlackoutGG/api.blackoutgg.com/issues"
  },
  "homepage": "https://github.com/BlackoutGG/api.blackoutgg.com#readme"
}
